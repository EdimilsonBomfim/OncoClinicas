                months.push($(this).attr('data-dtmonth'));
            });
            $('#dtmonths').val(months);

            if (months.length >= 12) {
                $('#all-dthmonth').prop('checked', true);
            } else {
                $('#all-dthmonth').prop('checked', false);
            }
